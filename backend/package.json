{
  "name": "backend",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "serve": "nodemon -r esm src/index.js",
    "lint": "eslint --ext .js",
    "test": "jest --verbose",
    "clean": "kill -9 $(lsof -t -i:4000)",
    "db:seed": "node -r esm src/db/seed.js",
    "db:clean": "node -r esm src/db/clean.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/delegate": "^7.0.7",
    "@graphql-tools/stitch": "^7.1.4",
    "apollo-datasource": "^0.7.2",
    "apollo-server": "^2.19.0",
    "bcrypt": "^5.0.0",
    "dotenv": "^8.2.0",
    "esm": "^3.2.25",
    "graphql": "^14.2.1",
    "graphql-middleware": "^4.0.2",
    "graphql-shield": "^7.4.2",
    "jsonwebtoken": "^8.5.1",
    "neo4j-driver": "^4.2.1",
    "neo4j-graphql-js": "^2.17.1"
  },
  "eslintConfig": {
    "env": {
      "node": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12
    },
    "rules": {
      "semi": [
        "error",
        "always"
      ],
      "quotes": [
        "error",
        "double",
        "avoid-escape"
      ]
    }
  },
  "jest": {
    "testMatch": [
      "**/src/**/**.spec.[jt]s?(x)"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "apollo-server-testing": "^2.19.0",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "nodemon": "^2.0.6"
  }
}