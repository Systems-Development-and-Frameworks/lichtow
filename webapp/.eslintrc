{
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  },
  "rules": {
    "quotes": ["error", "single"],
    "arrow-parens": ["error", "always"],
    "no-trailing-spaces": ["error"],
    "semi": ["error"],
    "max-len": ["error", 120],
    "object-curly-spacing": ["error", "always"]
  }
}
