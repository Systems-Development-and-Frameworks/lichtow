{
  "name": "hackernews",
  "version": "0.0.1",
  "author": "Jonas Zwink und Paula Osterkamp",
  "scripts": {
    "dev": "npm-run-all build serve",
    "serve": "cross-env NODE_ENV=dev webpack-dev-server",
    "lint": "prettier-eslint --write \"src/**/*.js\" \"**/*.vue\"",
    "build": "npm-run-all build:js resources",
    "build:js": "webpack --mode=production",
    "resources": "cp -r index.html public/"
  },
  "dependencies": {
    "vue": "2.6.10"
  },
  "devDependencies": {
    "@babel/core": "7.5.0",
    "@babel/preset-env": "7.5.2",
    "babel-loader": "8.0.6",
    "cross-env": "5.2.0",
    "css-loader": "3.0.0",
    "node-sass": "4.12.0",
    "npm-run-all": "4.1.5",
    "prettier-eslint-cli": "5.0.0",
    "sass-loader": "7.1.0",
    "vue-loader": "15.7.0",
    "vue-style-loader": "4.1.2",
    "vue-template-compiler": "2.6.10",
    "webpack": "4.35.3",
    "webpack-cli": "3.3.5",
    "webpack-dev-server": "3.7.2"
  }
}
